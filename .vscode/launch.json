{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Edge",
			"request": "launch",
			"type": "msedge",
			"url": "http://localhost:4200",
			"webRoot": "${workspaceFolder}/apps/client/"
		},
		{
			"name": "Launch Chrome",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost:4200",
			"webRoot": "${workspaceFolder}/apps/client/",
			"sourceMapPathOverrides": {
				"webpack:/*": "${webRoot}/*"
			}
		}
	]
}
